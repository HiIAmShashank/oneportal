# Example secrets file for local act testing
# Copy this to .secrets and fill in your actual values
# DO NOT COMMIT .secrets file (it's in .gitignore)

# Azure Static Web Apps deployment token
AZURE_STATIC_WEB_APPS_API_TOKEN=your-azure-swa-token-here

# Shared secrets
TENANT_ID=your-tenant-id-here
ADMIN_API_BASE_URL=https://your-api.azurewebsites.net

# Shell app secrets
SHELL_AUTH_CLIENT_ID=your-shell-client-id
SHELL_AUTH_REDIRECT_URI=http://localhost:5000/auth/callback
SHELL_AUTH_SCOPES=api://your-api-scope/access_as_user

# Domino app secrets
DOMINO_AUTH_CLIENT_ID=your-domino-client-id
DOMINO_AUTH_REDIRECT_URI=http://localhost:5173/auth/callback
DOMINO_AUTH_SCOPES=api://your-api-scope/access_as_user

# OnePortal Admin app secrets
ONEPORTAL_ADMIN_AUTH_CLIENT_ID=your-admin-client-id
ONEPORTAL_ADMIN_AUTH_REDIRECT_URI=http://localhost:5174/auth/callback
ONEPORTAL_ADMIN_AUTH_SCOPES=api://your-api-scope/access_as_user
